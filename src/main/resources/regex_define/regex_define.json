{
  "inputs": {
    "digit": "x >= '0' && x <= '9'",
    "char": "(x >= 'a' && x <= 'z') || (x >= 'A' && x <= 'Z')",
    "sign": "['>', '<', '!', '#', '+', '-', '*', '/', '&', '$', '@', '~'].indexOf(x) >= 0",
    "whitespace": "x == '\\n' || x == '\\r' || x == '\\t' || x == ' '",
    "ops": "['<' , '>' , '=' , '+' , '-' , '*' , '/'].indexOf(x) >= 0",
    "int_sign": "x == '+' || x == '-'",
    "exp": "x == 'e' || x == 'E'"
  },
  "Keyword": {
    "regex": "(begin, val, while, end, in, let, if, then, else)",
    "priority": 2
  }
}